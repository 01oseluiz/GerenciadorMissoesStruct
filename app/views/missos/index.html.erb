<p id="notice"><%= notice %></p>

<h1>Missos</h1>

<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th>Local</th>
      <th>Id nv</th>
      <th>Id equipe</th>
      <th>Id pessoa</th>
      <th>Preco</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @missos.each do |misso| %>
      <tr>
        <td><%= misso.nome %></td>
        <td><%= misso.local %></td>
        <td><%= misso.nv_dific_id %></td>
        <td><%= misso.equipe_id %></td>
        <td><%= misso.pessoa_id %></td>
        <td><%= misso.preco %></td>
        <td><%= link_to 'Show', misso %></td>
        <td><%= link_to 'Edit', edit_misso_path(misso) %></td>
        <td><%= link_to 'Destroy', misso, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Misso', new_misso_path %>
