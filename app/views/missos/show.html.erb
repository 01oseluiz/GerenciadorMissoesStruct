<p id="notice"><%= notice %></p>

<p>
  <strong>Nome:</strong>
  <%= @misso.nome %>
</p>

<p>
  <strong>Local:</strong>
  <%= @misso.local %>
</p>

<p>
  <strong>Id nv:</strong>
  <%= @misso.nv_dific_id %>
</p>

<p>
  <strong>Id equipe:</strong>
  <%= @misso.equipe_id %>
</p>

<p>
  <strong>Id pessoa:</strong>
  <%= @misso.pessoa_id %>
</p>

<p>
  <strong>Preco:</strong>
  <%= @misso.preco %>
</p>

<br/>

<h2>Comentarios:</h2>
<%= render @misso.comentarios %>

<br/>

<% if current_pessoa.tipo.downcase == "admin" %>
    <%= link_to 'Editar', edit_misso_path(@misso) %> |
    <%= link_to 'Deletar', @misso, method: :delete, data: { confirm: 'Are you sure?' } %> |
<% end %>
<%= link_to 'Fazer Comentario', new_misso_comentario_path(@misso) %> |
<%= link_to 'Voltar', :back %>
