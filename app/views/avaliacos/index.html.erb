<p id="notice"><%= notice %></p>

<h1>Avaliacos</h1>

<table>
  <thead>
  <tr>
    <th>Avaliacao</th>
    <th>Id avaliado</th>
    <th>Id avaliador</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @avaliacos.each do |avaliaco| %>
      <tr>
        <td><%= avaliaco.avaliacao %></td>
        <td><%= avaliaco.avaliado_id %></td>
        <td><%= avaliaco.avaliador_id %></td>
        <td><%= link_to 'Show', avaliaco %></td>
      </tr>
  <% end %>
  </tbody>
</table>
